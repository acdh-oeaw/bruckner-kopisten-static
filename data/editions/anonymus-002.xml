                  <floruit from="1862" to="1885" xml:lang="eng">1862–1863, 1866–1870 and
                     1878–1885</floruit>
                  <residence>Linz</residence>
                  <affiliation xml:lang="de">Linzer Kopisten</affiliation>
                  <affiliation xml:lang="eng">Linz copyist</affiliation>
